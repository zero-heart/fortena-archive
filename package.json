{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run sitemap && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "sitemap": "node scripts/generate-sitemap.js",
    "prepare-gh-pages": "rm -rf dist && cp -r .output/public dist && echo > dist/.nojekyll",
    "deploy": "npm run build && npm run prepare-gh-pages && gh-pages -d dist"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.9.0",
    "@vuepic/vue-datepicker": "^11.0.1",
    "nuxt": "^3.15.4",
    "pinia": "^2.3.1",
    "vue": "latest",
    "vue-router": "latest",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "sass": "^1.84.0"
  }
}
